msgid "$d.npcChat.firewood.answer.com2"
msgstr "Daj spokój, przesadzasz."


msgid "$d.npcChat.firewood.answer.rich1"
msgstr "Niewiarygodne! Musisz coś z tym zrobić!"


msgid "$d.npcChat.firewood.answer.rich2"
msgstr "Hańba! Nie można akceptować takich rzeczy! Zrób coś z tym!"


msgid "$d.npcChat.shadows.line.poor1"
